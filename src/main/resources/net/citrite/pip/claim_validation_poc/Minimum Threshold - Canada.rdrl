package net.citrite.pip.claim_validation_poc;

import java.lang.Number;
import net.citrite.pip.canonical.bre.BREObjectSFDC;
import net.citrite.pip.sfdc.Request;
import net.citrite.pip.sfdc.EndUserInfo;
import net.citrite.pip.sfdc.Order;
import net.citrite.pip.sfdc.OrderHeader;
import net.citrite.pip.sfdc.OrderLineItem;

rule "Minimum Threshold - Canada" extends "Is Follow On Order"
	dialect "mvel"
	when
		bre : BREObjectSFDC( request.order.orderTotalSRP >= 2500 , request.order.orderTotalSRP < 10000 , request.order.orderHeader.order_Creation_Date > "31-Jan-2015" )
	then
end
